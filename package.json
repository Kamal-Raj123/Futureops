{
  "name": "prediction-power-platform",
  "private": true,
  "version": "1.2.0",
  "description": "A comprehensive AI-powered prediction platform for weather, stocks, climate, elections, and global strategies",
  "type": "module",
  "keywords": [
    "ai",
    "machine-learning",
    "predictions",
    "forecasting",
    "tensorflow",
    "react",
    "typescript",
    "supabase"
  ],
  "author": "Prediction Power Team",
  "license": "MIT",
  "homepage": "https://predictionpower.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-github-username/prediction-power-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-github-username/prediction-power-platform/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "supabase gen types typescript --local > src/types/database.ts",
    "db:reset": "supabase db reset --local",
    "prepare": "husky install",
    "setup": "node scripts/setup-project.js",
    "update-vars": "chmod +x scripts/update-variables.sh && ./scripts/update-variables.sh",
    "validate-config": "node scripts/validate-config.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@tensorflow/tfjs": "^4.15.0",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^3.0.6",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.1.0",
    "supabase": "^1.142.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}