# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Optional: External API Keys for Enhanced Features
# Weather API (for real weather data)
VITE_WEATHER_API_KEY=your_weather_api_key

# Stock Market API (for real financial data)
VITE_STOCK_API_KEY=your_stock_api_key

# News API (for sentiment analysis)
VITE_NEWS_API_KEY=your_news_api_key

# OpenRouter API (for premium LLM models)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key

# Development Configuration
NODE_ENV=development
VITE_APP_ENV=development

# Analytics (optional)
VITE_ANALYTICS_ID=your_analytics_id

# Deployment Configuration
NETLIFY_SITE_ID=your_netlify_site_id
NETLIFY_AUTH_TOKEN=your_netlify_auth_token

# Database Configuration (for local development)
DATABASE_URL=postgresql://username:password@localhost:5432/prediction_power

# Redis Configuration (for caching)
REDIS_URL=redis://localhost:6379

# Email Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password

# File Storage (for model uploads)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_BUCKET_NAME=your_s3_bucket_name
AWS_REGION=us-east-1

# Monitoring and Logging
SENTRY_DSN=your_sentry_dsn
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Feature Flags
ENABLE_DEMO_MODE=true
ENABLE_TRAINING=true
ENABLE_REAL_TIME_UPDATES=true
ENABLE_ANALYTICS=true

# API Endpoints
VITE_API_BASE_URL=http://localhost:3000/api
VITE_WS_URL=ws://localhost:3000

# Model Configuration
DEFAULT_MODEL_TIMEOUT=30000
MAX_PREDICTION_BATCH_SIZE=100
MODEL_CACHE_TTL=3600

# UI Configuration
VITE_APP_NAME="Prediction Power Platform"
VITE_APP_DESCRIPTION="AI-powered prediction platform"
VITE_APP_VERSION=1.2.0
VITE_SUPPORT_EMAIL=support@predictionpower.com

# Social Media (for sharing)
VITE_TWITTER_HANDLE=predictionpower
VITE_GITHUB_REPO=your-username/prediction-power-platform

# Backup Configuration
BACKUP_SCHEDULE="0 2 * * *"
BACKUP_RETENTION_DAYS=30

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_SAMPLE_RATE=0.1

# Content Security Policy
CSP_REPORT_URI=https://your-domain.report-uri.com/r/d/csp/enforce

# CORS Configuration
CORS_ORIGIN=http://localhost:5173
CORS_CREDENTIALS=true

# Session Configuration
SESSION_SECRET=your_session_secret
SESSION_TIMEOUT=86400000

# Webhook Configuration
WEBHOOK_SECRET=your_webhook_secret
WEBHOOK_TIMEOUT=5000

# Machine Learning Configuration
ML_MODEL_PATH=./models
ML_TRAINING_TIMEOUT=3600000
ML_MAX_TRAINING_JOBS=5

# Notification Configuration
ENABLE_EMAIL_NOTIFICATIONS=true
ENABLE_PUSH_NOTIFICATIONS=false
NOTIFICATION_QUEUE_SIZE=1000

# Internationalization
DEFAULT_LOCALE=en
SUPPORTED_LOCALES=en,es,fr,de,zh

# Development Tools
ENABLE_DEVTOOLS=true
ENABLE_HOT_RELOAD=true
ENABLE_SOURCE_MAPS=true